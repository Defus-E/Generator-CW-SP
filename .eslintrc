{
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "camelcase": "off",
    "no-template-curly-in-string": "off",
    "no-constant-condition": "off",
    "no-await-in-loop": "off",
    "no-useless-escape": "off"
  }
}